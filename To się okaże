#include <stdio.h>
#include <math.h>

int main(void)
{
float a,b,c,x1,x2,d;

printf("Podaj a, b i c z rÃ³wnania kwadratowego ax^2+bx+c\n");
scanf("%f %f %f", &a, &b, &c);

d = sqrt(b*b-4*a*c);

if(d==0)
x1=(-b)/2*a;

if(d>0)
{
x1=((-b)-d)/(2*a);
x2=((-b)+d)/(2*a);
}
if(d<0)
printf("Brak miejsc zerowych. Delta mniejsza od 0.\n");

return 0;
}
